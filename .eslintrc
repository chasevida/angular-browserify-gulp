{
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "_": true,
        "angular": true,
        "FastClick": true
    },
    "rules": {
        "strict": 0,
        "brace-style": [2, "stroustrup"],
        "new-cap": 0,
        "camelcase": 1,
        "semi": [2, "always"],
        "quotes": [2, "single"],
        "consistent-this": [2, "self"],
        "func-style": [2, "expression"],
        "no-loop-func": 2,
        "new-parens": 2,
        "no-array-constructor": 2,
        "no-lonely-if": 2,
        "no-new": 2,
        "no-new-object": 2,
        "no-spaced-func": 2,
        "no-space-before-semi": 2,
        "no-trailing-spaces": 1,
        "no-underscore-dangle": 0,
        "no-mixed-spaces-and-tabs": 2,
        "sort-vars": 1,
        "space-after-keywords": 2,
        "space-in-brackets": [1, "never"],
        "space-infix-ops": 2,
        "space-in-parens": 2,
        "space-return-throw-case": 2,
        "space-unary-word-ops": 2,
        "no-unused-vars": 1,
        "no-use-before-define": 2,
        "eqeqeq": 2,
        "curly": [2, "all"],
        "dot-notation": 1,
        "no-eval": 2,
        "no-implied-eval": 2,
        "no-else-return": 2,
        "no-eq-null": 1,
        "no-extend-native": 1,
        "yoda": [1, "never"],
        "no-return-assign": 2,
        "no-new-wrappers": 2,
        "no-native-reassign": 2,
        "no-constant-condition": 2,
        "no-comma-dangle": 2,
        "no-sparse-arrays": 2,
        "no-unreachable": 2,
        "no-ex-assign": 2,
        "block-scoped-var": 2,
        "no-reserved-keys": 2,
        "no-unused-vars": 2,
        "no-shadow": 1,
        "no-undef": 2,
        "valid-jsdoc": 1,
        "max-nested-callbacks": [2, 3]
    }
}
